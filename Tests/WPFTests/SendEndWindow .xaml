<Window x:Class="WPFTests.SendEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTests"
        mc:Ignorable="d"
        Title="SendEndWindow" Height="175" Width="400" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label x:Name="lSendEnd" Content="Работа завершена" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" FontSize="36">
        </Label>
        <Button x:Name="btnOK" Content="OK" Margin="0,0,50,20" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="57" Click="btnOK_Click"/>

    </Grid>
</Window>
